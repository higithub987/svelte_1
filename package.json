{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/runtime": "^7.7.2",
    "nollup": "^0.8.2",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-hot": "^0.0.4-0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-svelte-hot": "^0.0.8",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0",
    "svelte-filerouter": "rixo/svelte-filerouter#codesplit"
  },
  "dependencies": {
    "sirv-cli": "^0.4.4"
  },
  "scripts": {
    "build": "rollup -c",
    "dev:rollup": "rollup -c -w",
    "dev:livereload": "LIVERELOAD=1 rollup -c -w",
    "dev:nollup": "NOLLUP=1 nollup -c --hot --port 5000 --content-base ./public",
    "dev": "npm run dev:rollup",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev"
  }
}
